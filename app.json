{
  "expo": {
    "name": "FC-Services-Native",
    "description": "FC Services é uma aplicação para facilitar a vida dos funcionarios da Ferreira Costa, com ele é possível solicitar serviços de forma rápida e fácil.",
    "owner": "SQUAD 1",
    "privacy": "public",
    "slug": "FC-Services-Native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/adaptive-icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "project.ext.react": {
        "enableHermes": true
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "githubUrl": "https://github.com/SQUAD-1/FC-Services-Native",
    "jsEngine": "hermes"
  }
}
